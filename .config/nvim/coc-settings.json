{
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "tsserver.npm": "/usr/local/bin/npm",
  "tsserver.enable": true,
  "tsserver.maxTsServerMemory": 2000,
  "tsserver.locale": "ja",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "solargraph.checkGemVersion": false,
  "solargraph.commandPath": "~/.asdf/shims/solargraph",
  "solargraph.diagnostics": false,
  "solargraph.references": true,
  "coc.preferences.formatOnSave": true,
  "coc.preferences.formatOnSaveTimeout": 5000,
  "importCost.typescriptExtensions": ["\\.tsx?$"],
  "importCost.javascriptExtensions": ["\\.jsx?$"],
  "importCost.bundleSizeDecoration": "both",
  "importCost.showCalculatingDecoration": true,
  "importCost.debug": true,
  "codeLens.enable": true,
  "javascript.referencesCodeLens.enable": false,
  "coc.preferences.useQuickfixForLocations": false,
  "suggest.highPrioritySourceLimit": 10,
  "suggest.languageSourcePriority": 99,
  "snippets.priority": 90,
  "suggest.lowPrioritySourceLimit": 5,
  "suggest.selection": "recentlyUsedByPrefix",
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "coc.source.file.priority": 10,
  "coc.source.around.priority": 10,
  "coc.source.buffer.priority": 10,
  "diagnostic.signPriority": 10,
  "suggest.removeDuplicateItems": true,
  "snippets.userSnippetsDirectory": "~/.config/snippets",
  "snippets.snipmate.enable": true,
  "sqlfluff.formatIgnoreStderrAlert": false,
  "sqlfluff.lintOnSave": true,
  "json.validate.enable": true,
  "json.format.enable": false,
  "coc.preferences.jumpCommand": "drop",
  "pyright.organizeimports.provider": "isort",
  "python.formatting.provider": "black",
  "python.sortImports.path": "isort",
  "python.linting.flake8Enabled": false,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportIncompatibleMethodOverride": "warning"
  },
  "codeLens.position": "eol",
  "diagnostic.enableHighlightLineNumber": false,
  "rust-analyzer.procMacro.enable": true,
  "rust-analyzer.diagnostics.disabled": ["unresolved-import"],
  "svelte.enable-ts-plugin": true,
  "yaml.validate": true,
  "yaml.format.singleQuote": true,
  "yaml.schemas": {
    "https://raw.githubusercontent.com/composer/composer/master/res/composer-schema.json": [
      "/*"
    ],
    "https://json.schemastore.org/kustomization": "/kustomization.yaml",
    "kubernetes": ["*/*.yaml"],
    "https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json": [
      "docker-compose*.yaml",
      "docker-compose*.yml",
      "compose*.yaml",
      "compose*.yml"
    ],
    "https://json.schemastore.org/github-workflow.json": ".github/workflows/*.yml"
  },
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    }
  },
  "diagnostic.checkCurrentLine": true,
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck",
    "yaml": ["actionlint"]
  },
  "diagnostic-languageserver.formatFiletypes": {
    "sh": "shfmt"
  },
  "diagnostic-languageserver.formatters": {
    "shfmt": {
      "command": "shfmt",
      "args": ["-i", "2", "-bn", "-ci", "-sr"]
    }
  },
  "diagnostic-languageserver.linters": {
    "actionlint": {
      "command": "actionlint",
      "rootPatterns": [".github"],
      "debounce": 100,
      "isStdout": true,
      "isStderr": false,
      "offsetLine": 0,
      "offsetColumn": 0,
      "args": ["-oneline"],
      "sourceName": "actionlint",
      "formatLines": 1,
      "formatPattern": [
        "^([^:]+):(\\d+):(\\d+):\\s+(.*)$",
        {
          "sourceName": 1,
          "sourceNameFilter": true,
          "line": 2,
          "column": 3,
          "endLine": 2,
          "endColumn": 3,
          "message": [4]
        }
      ]
    }
  },
  "snippets.ultisnips.pythonPrompt": false
}
