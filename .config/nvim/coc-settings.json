{
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "tsserver.npm": "/usr/local/bin/npm",
  "tsserver.enableJavascript": true,
  "tsserver.enable": true,
  "tsserver.maxTsServerMemory": 2000,
  "tsserver.locale": "ja-jp",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "solargraph.checkGemVersion": false,
  "solargraph.commandPath": "/usr/local/lib/ruby/gems/2.7.0/bin/solargraph",
  "solargraph.diagnostics": false,
  "solargraph.references": true,
  "importCost.typescriptExtensions": [
    "\\.tsx?$"
  ],
  "importCost.javascriptExtensions": [
    "\\.jsx?$"
  ],
  "importCost.bundleSizeDecoration": "both",
  "importCost.showCalculatingDecoration": true,
  "importCost.debug": true,
  "codeLens.enable": true,
  "javascript.referencesCodeLens.enable": false,
  "coc.preferences.colorSupport": true,
  "coc.preferences.useQuickfixForLocations": false,
  "highlight.document.enable": true,
  "highlight.colors.enable": true,
  "highlight.disableLanguages": ["php","vim", "fugitive"],
  "phpactor.path": "~/.vim/plugged/phpactor/bin/phpactor"
}
